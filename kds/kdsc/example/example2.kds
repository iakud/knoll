package kds;

entity City {
	int64 PlayerId = 1;
	PlayerBasicInfo PlayerBasicInfo = 2;
	CityBaseInfo CityInfo = 3;
	repeated int64 Troops = 4;
}

component CityBaseInfo {
	repeated Vector Positions = 1;
	map<int32, empty> Troops = 2;
	bytes BuildInfo = 3;
}

component Vector {
	int32 X = 1;
	int32 Y = 2;
}